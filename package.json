{
  "name": "arroyo-erp-client",
  "author": "Eduardo Parra Mazuecos",
  "email": "eduparra90@gmail.com",
  "licence": "MIT",
  "version": "2.6.3",
  "private": true,
  "scripts": {
    "lint": "eslint ./src",
    "start": "react-scripts start .env.development",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "publish": "docker build . -t soker90/arroyo-erp-client:latest -f ./Dockerfile && docker push soker90/arroyo-erp-client:latest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.8",
    "@mui/styles": "^5.10.8",
    "@mui/x-date-pickers": "^5.0.4",
    "axios": "^1.1.2",
    "chart.js": "^3.9.1",
    "clsx": "^1.2.1",
    "history": "^5.3.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-helmet": "^6.1.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^8.0.4",
    "react-redux-loading-bar": "^5.0.4",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  }
}
